{
  "name": "keq",
  "displayName": "Keq",
  "description": "Keq for VsCode",
  "version": "0.0.1",
  "publisher": "keq-request",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/.keqrc{,.json,.ts,.js,.cjs,.mjs,.yml,.yaml}",
    "workspaceContains:**/keq.config.{ts,js,cjs,mjs,json,yml,yaml}"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "keq.initConfig",
        "title": "Initialize Keq Config"
      },
      {
        "command": "keq.installCli",
        "title": "Install @keq-request/cli"
      },
      {
        "command": "keq.generate",
        "title": "Generate Code for Operation",
        "icon": "$(sync)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "keq-explorer",
          "title": "Keq",
          "icon": "images/logo.svg"
        }
      ]
    },
    "views": {
      "keq-explorer": [
        {
          "id": "keqExplorerWebview",
          "name": "APIs",
          "type": "webview"
        }
      ]
    },
    "menus": {}
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.107.0",
    "@vscode-elements/elements": "^2.4.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  },
  "packageManager": "pnpm@10.26.2+sha512.0e308ff2005fc7410366f154f625f6631ab2b16b1d2e70238444dd6ae9d630a8482d92a451144debc492416896ed16f7b114a86ec68b8404b2443869e68ffda6"
}
